// Write a c program to convert decimal number to binary.


#include <stdio.h>
void binary(int n)
{
    int a[100];
    int i=0;
    for(i=0;n>0;i++)    
{    
a[i]=n%2;    
n=n/2;    
}    
printf("\nBinary of Given Number is=");    
for(int j=i-1;j>=0;j--)    
{    
printf("%d",a[j]);    
}  
}
int main()
{
    int n;
    printf("Enter the number: ");
    scanf("%d",&n);
    binary(n);

    return 0;
}
