// Write a c program to find factorial of n using function

#include<stdio.h>
int fact(int n)
{
    int fact=1;
    for(int i=1;i<=n;i++)
    fact *=i ;
    
    return fact;
}
int main()
{
    int num;
    printf("Enter n: ");
    scanf("%d",&num);
    printf("Fcatorial of %d is %d\n",num,fact(num));
    return 0;
}
