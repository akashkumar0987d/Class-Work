//Largest of 2 num

#include<stdio.h>

void main()
{
      int a,b;
      printf("Enter Number 1 :");
      scanf("%d",&a);
      printf("\nEnter Number 2 :");
      scanf("%d",&b);      
      if(a>b)
      {

         printf("\nNumber 1 is greater");

      }
      
      else
      {
            printf("\nNumber 2 is greater");
      }

}
