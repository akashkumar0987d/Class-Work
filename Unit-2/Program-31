// Write a c program to swap the number using function

#include <stdio.h>
  
void swap(int a, int b);

int main()
{
    int m=22,n=14;
    printf("vakue before swapping is m=%d and n=%d\n",m,n);
    swap(m,n);
    
    return 0;
}
void swap(int a, int b)
{
    int temp;
    temp = a;
    a = b;
    b = temp;
    printf("Vlaue after swapping is m=%d  and n=%d\n",a,b);
}
