// Write a c program to find factorial of n using while loop

#include <stdio.h>

int main()
{
    int n;
    printf("Enter the value of n: ");
    scanf("%d",&n);
    int i=1,fact=1;
    while(i<=n)
    {
        fact = fact*i;
        i++;
    }
    printf("The factorial of %d is %d",n,fact);
    return 0;
}
